{
  "name": "powelectrics-firewall_framework",
  "version": "2.0.0",
  "author": "PowTechnology Limited",
  "summary": "Adds pre- and post- rules to implement the chains described in puppet-firewall",
  "license": "proprietary",
  "source": "https://github.com/powtechnology/puppet-firewall_framework",
  "project_page": "https://github.com/powtechnology/puppet-firewall_framework",
  "issues_url": "https://github.com/powtechnology/puppet-firewall_framework/issues",
  "dependencies": [
    {
      "name": "puppetlabs/firewall",
      "version_requirement": ">= 1.0.0 < 8.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "20.04",
        "22.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.0 < 9.0.0"
    }
  ]
}
